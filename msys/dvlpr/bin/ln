#!/bin/sh
# Copyright (C) 2002, Earnie Boyd
#   mailto:earnie@users.sf.net
# This file is part of Minimal SYStem
#   http://www.mingw.org/msys.shtml
# File: ln - Win32 replacement for symlink.

if [ $1 = '-s' ]
then
    shift
fi

if [[ $# -eq 1 ]]
then
  FROM=$1
  TO=`basename $FROM`
else
  while [[ $# -gt 1 ]]
  do
    FROM="$FROM$1"
    if [[ $# -gt 2 ]]
    then
      FROM="$FROM "
    fi
    shift
  done
  TO=$1
fi

cp -a $FROM $TO
