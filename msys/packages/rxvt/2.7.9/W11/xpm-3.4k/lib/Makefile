OS_NAME_DEFINES =

# $TOG: Library.tmpl /main/44 1997/04/28 11:47:53 kaleb $

         CC = gcc
  CCOPTIONS =
STD_DEFINES = -D__i386__ -DX_LOCALE -D_MT -DNO_TCP_H $(PROJECT_DEFINES)
